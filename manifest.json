{
  "manifest_version": 2,

  "name": "SpeedUp",
  "description": "Share easily",
  "version": "2.1",

  "browser_action": {
    "default_icon": "Images/icon.ico",
    "default_popup": "html/popup.html"
  },

  "background": {
          "persistent": false,
          "scripts": ["js/background.js"]
      },

  "permissions": ["<all_urls>", "contextMenus", "storage", "activeTab"]
}
 